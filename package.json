{
  "name": "dev-sheet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "firebase": "^4.13.1",
    "prop-types": "^15.6.1",
    "re-base": "^3.2.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "watch": "concurrently --names \"webpack, stylus\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "styles": "stylus -u autoprefixer-stylus ./src/css/styles.styl -o ./src/css/styles.css",
    "styles:watch": "stylus -u autoprefixer-stylus -w ./src/css/styles.styl -o ./src/css/styles.css"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "react-scripts": "^1.1.4"
  }
}
